.flow{
	display: -moz-box; 
    display: -webkit-box; 
    display: box;
    width: 100%;
    margin-top: 100px;
}
.stepwrap{
	position: relative;
}
.step{
	width: 240px;
    height: 150px;
    border:1px solid #4c4c4c;
    font-size: 18px;
    color: #333;
    border-radius: 20px;
    padding:30px 20px;
    vertical-align: middle;
}
.stepwrap img{
	position: absolute;
	display: inline-block;
	width: 100px;
	height: 100px;
	border-radius: 100px;
	top:-50px;
	left: 70px;
}
