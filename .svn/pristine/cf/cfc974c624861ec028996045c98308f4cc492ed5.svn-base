/*搜索导航栏*/
.row-bt{
	margin-top: 16px;
}
.row-bs{
	margin-top:30px;
}
.row .img-wrap{
	position: relative;
}
.row .title-wrap{
	position: absolute;
	bottom:0px;
	padding: 10px;
	margin:0;
	font-size: 24px;
	width: 100%;
	background-color: rgba(0,0,0,0.3);
	-webkit-transition: height 1s;
	height: 50px;
	overflow-y: hidden;
	color: #fff;
}
.title-wrap:hover{
	height: 90px;
	color: #fff;
}
a:hover .title-wrap{
	color:#fff;
}

.title-wrap .title{
	text-align: right !important;
}
.title-wrap .ps{
	font-weight: normal;
	text-align: center;
	font-size:20px;
}
.title-sm .ps{
	font-size: 16px;
}
.row .title-sm{
	font-size: 16px;
	height: 40px;
	-webkit-transition: height 1s;
}
.title-sm:hover{
	height: 80px;
}
.title-sm-m:hover{
	height: 90px;
}
