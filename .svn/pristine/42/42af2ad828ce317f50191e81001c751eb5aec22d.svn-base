<script>
$(function(){
    $("#landBtn,#startPay").on("click",function(){
    	$("#myModal").show();
    	$("#login_modal").show();   
    	   
    	$("#myModal").on("click",function(){
    		$(this).hide();
    		$("#login_modal").hide();   
    	});
    	 
    });
    $("#fogtPwd_btn").on("click",function(){
    	$("#login_modal").hide();
    	$("#aginresetPwd").show();
    	
    	$("#myModal").on("click",function(){
    		$(this).hide();
    		$("#aginresetPwd").hide();   
    	});
    });
    $("#regsiter_btn").on("click",function(){
    	$("#telReg").show();
    	  $("#reg_login").on("click",function(){
    			$("#telReg").hide();
    	    	$("#login_modal").show();
    	    	
    	    	$("#myModal").on("click",function(){
    	    		$(this).hide();
    	    		$("#login_modal").hide();   
    	    	});
    	    });
    	  
	    	$("#myModal").on("click",function(){
	    		$(this).hide();
	    		$("#telReg").hide();   
	    	});
    });
        $(".dropmenu").click(function(){
      $(this).children('.dropitem_phone').toggle();
    })
    //生成手机验证码的
    $("#createStr").on("click",function(){
    	var username = $("#qwqwqw").val();
		var password = $("select[name=phonetitle]").val();
		alert(username);
    	$.post("createStr.do",{phone:username,phonetitle:password},function(data){
			if(data.success){
				$("#login_modal").show();
			}
    	});
    
 });
 $(".news").click(function(){
  $(location).attr('href','./weixin_page.html')
 })
    

$(function(){
	$("#loginform").ajaxForm(function(data){
		if(data.success){
		setTimeout(function(){
			location.href="index.jsp";
		},800)
		}
	});
});
$(function(){
	$("#insertUser").ajaxForm(function(data){
		if(data.success){
		setTimeout(function(){
			$("#login_modal").show();
		},800)
		}
	});    
});
});
// 更多服务项目显示
$("#moreserveitem").click(function(){
  $(".moreserveitem").toggle();
  if($('.moreserveitem').css("display") == "block"){
      $("#services .icon_img_row ").css("border-bottom","1px solid black");
  }
  else{
      $("#services .icon_img_row ").css("border-bottom","none");
  }
})


</script>

