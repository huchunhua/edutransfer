form{
	width: 100%;
	margin-top: 0px;
	margin-bottom: 0px;
	padding: 20px 0 100px;
	background-color:inherit;
	display: none;
	position: relative;
}
.form-horizontal .form-group{
	display: none;
}
.info_box{
	margin:10px auto;
	padding-bottom: 30px;
	font-size: 14px;

}
.info_box>p{
	font-size: 18px;
	font-weight: 700;
	margin-bottom: 20px;
}
.info_box .tips{
	color: rgb(240,90,36);
    font-size: 14px;
	
}
.info_box>.user_name{
	margin-top: 30px;
}
.student_number i{
	background-image: url("../img/tubiao06.png");
}
.country i,.nationnality>i{
	background-image: url("../img/tubiao12.png");
}
.school i{
	background-image: url("../img/tubiao07.png")
}
.info_box table{
	width: 100%;
	margin: 20px 0;
}
.info_box tr{
	padding: 5px;
	border-bottom: 2px solid #B0C4DE;
}
.info_box th{
	font-size: 16px;
	height: 60px;
	background-color: #EDEDED;

}
.info_box td{
	height: 60px;
	padding: 10px;
}
.info_box button{
	width: 110px;
	height: 30px;
	border-radius: 4px;
	color: #fff;
	border:1px;
	text-align: center;
}
.info_box button.rbtn,form button.rbtn{
	background-color: #dc2b2b;
	border: 1px solid #dc2b2b;
}
.info_box button#add_btn{
	width: 130px;
/*	text-align: left;*/
	position: relative;
}
form button{
	display: none;
}
.info_box ul.additem{
	display: none;
	list-style: none;
	padding-left: 0;
	border: 1px solid #3879d9;
	width: 130px;
	overflow:hidden; 
	white-space:nowrap;
	text-overflow:ellipsis;
	position: absolute;
	top:26px;
	left:-1px;
	border-top:none;
	border-radius:0 4px 4px 0;
}
.additem li{
	color: #3879d9;
	background-color:#fff ;
	padding:10px 0;
	padding-left:4px;
}
.additem li:hover{
	color:#fff;
	background-color:  #3879d9;
}
.additem li:first-child{
	padding-top: 14px;
}
.info_box .service_side{
	width:300px;
	margin-top:-30px;
	display:block;
}
.info_box #pay_btn{
	float: right;
}
.info_box .chart{
	float: right;
	margin-right: 10px;
}
.bgimg{
	width: 100%;
	height: 200px;
	background-image: url("../img/banner-infermation.png");
	background-size: cover;
}
.content{
	width: 1170px;
	min-width: 900px;
	margin:0 auto;
}
.hide_information{
    width:520px; 
	height:620px; 
	background-color:#fff; 
	border-radius:5px; 
	position:fixed; 
	top:130px;left:0; 
	margin-left:38%;
	padding:20px 20px;
	border:solid 1px #C1C1C1;
	z-index:100;
	display:none;
}
.ruzhu{ 
	text-align:center;
	background-color:#067a0c;
	color:#fff;
	border-radius:4px;
	line-height:60px;
}
.content_information_box{
	padding:15px 0 15px 10px;
	width:440px;
	position:relative;	 
}
.start_information{ 
	float:left; 
	font-size:16px; 
	margin-right:40px;
	line-height:34px;
	padding-left:32px;
	
}
.start_information01{ 
	float:left; 
	font-size:16px; 
	line-height:34px;
	
}
.start_left{
	margin-left:-32px;
	
}
.content_information{
	width:290px;
	color:#999;
	
}
#select_box{
	margin-left:40px;
	width:290px;
	padding:5px 0;
	border-radius:4px;
	color:#999;
}
#up{
	padding:5px 0;
}
.top_content{
	padding-top:10px;
	padding-left:140px;
}
.sumb{ 
	width:70px;
	line-height:34px;
	background-color:#D41A1D;
	border-radius:6px;
	border:0;
	color:#FFFFFF;
	font-size:18px;
	margin-top:20px;
	margin-left:50px;
}
.last_content{
	display:none;
	text-align:center;
	padding:70px 0;
}
.last_content p{
	font-size:16px;
	margin-top:20px;
	line-height:30px;
}
.ok{ 
	width:25%; 
	margin-bottom:20px;
}
.op{ 
	opacity:0.5;
}
.marg{
	margin-bottom:10px;
}
.submit{
	text-align: center;
}
.reminderimg{
	display: none;
}
.location01{
	position: absolute;
    right: 0;
    top: 20px;
}
.location04{
	position: absolute;
    right: 0;
    top: 130px;
}
.tri{
	content: "";
    display: inline-block;
    position: absolute;
    top:12px;
    right: 14px;
    bottom: auto;
	border-left: 6px solid #3879d9;
    border-right: 6px solid #3879d9;
    border-top: 6px solid #fff;
    border-bottom: 0;
}
#add_btn:hover .tri{
	border-left: 6px solid #589afc;
    border-right: 6px solid #589afc;
}