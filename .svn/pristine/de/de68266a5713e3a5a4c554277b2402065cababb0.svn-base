@media screen and (max-width: 1204px) {
    .continent{margin:10px 0;}
}

@media screen and (max-width: 1024px) {
    #testimonials .carousel-control {
        display:block
    }
    .continent{margin:10px 0;}
}

@media screen and (max-width: 991px) {
    #portfolio .items li {
        width:50%
    }

    #testimonials .quote {
        padding: 0 70px
    }
}

@media(max-width: 767px) {
    .page .content.cover {
        padding:160px 0
    }

    #testimonials .quote {
        padding: 0 50px
    }

    #testimonials .carousel-control.right {
        right: 10px
    }


    .selector select {
        width: 100%;
        -webkit-border-radius: 6px;
        -moz-border-radius: 6px;
        border-radius: 6px;
        padding-left: 10px
    }

    .foot_about {
        text-align: center;
        margin: 20px auto 30px
    }

    .foot_about>a {
        display: inline-block;
        width: 100%;
        padding-right: 3px
    }

    .bor {
        width: 80%;
        margin-top: 80px
    }

    #main {
        height: 450px
    }
    #transfer{
        height: 68px;
        top:382px;
    }
    .page .content.cover {
        padding: 80px 0
    }

    .icon_img {
        margin: 0 auto 40px
    }
   .dropmenu .dropitem_phone{
        display: block;
        padding-left: 30px;
        font-weight: normal;
   }
   .dropitem_phone a{
    color: #fff;
    padding-left: 40px;
   }
   .dropmenu:hover .dropitem,.dropmenu:hover .dropitem_land{
        display: none !important;
   }
   .navbar-nav>li>a.news{
    margin-left: 10px;
   }
}

@media(max-width: 570px) {
    .icon_img {
        margin:0 auto 60px
    }
}

@media(max-width: 520px) {
    .search_area select,.search_area input,.search_area button {
        /*width:100%;*/
        margin-bottom: 15px
    }

    .search_area button {
        margin-left: 0
    }
}

@media(max-width: 480px) {
    .page .content.cover {
        padding:80px 0
    }

    #portfolio .items li {
        width: 100%
    }

    #testimonials .quote {
        padding: 0 30px
    }

    .bor {
        width: 90%;
        margin-top: 50px
    }

    #main .btnLeft,#main .btnRight {
        display: none
    }
}

@media(max-width: 450px) {
    #main {
        height:350px
    }

    .bor {
        margin-top: 40px
    }
    #transfer{
        position: static;
    }
}

@media screen and (max-width: 350px) {
    .tit_txt h1 {
        font-size:50px
    }

    .find_more {
        font-size: 50px;
        line-height: 30px
    }

    .banner_tit {
        font-size: 20px
    }

    .carousel-control.left,.carousel-control.right {
        width: 60px;
        height: 60px;
        font-size: 50px
    }
}

@media screen and (max-width: 340px) {
    .icon_img {
        margin:0 auto 70px
    }
}

@media screen and (max-width: 300px) {
    .tit_txt h1 {
        font-size:42px
    }

    .find_more {
        font-size: 40px;
        line-height: 30px
    }

    .banner_tit {
        font-size: 19px
    }
}